    "title": "Ukudibana Kokuqala",
    "entries": [
        {
            "text": "Molo!",
            "speaker": "A"
        },
        {
            "text": "Molo. Ndiyavuya ukukwazi.",
            "speaker": "B"
        },
        {
            "text": "Ngubani igama lakho?",
            "speaker": "A"
        },
        {
            "text": "Igama lam ngu [Igama]. Wena?",
            "speaker": "B"
        },
        {
            "text": "Igama lam ngu [Igama]. Uvela phi?",
            "speaker": "A"
        },
        {
            "text": "Ndivela e-Amerika. Wena?",
            "speaker": "B"
        },
        {
            "text": "Ndivela e-Korea. Ndiyakujabulela ukukwazi.",
            "speaker": "A"
        },
        {
            "text": "Nam ngokunjalo. Hamba kakuhle.",
            "speaker": "B"
        }
