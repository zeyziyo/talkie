{
    "title": "Ukuhlangana Kokuqala",
    "entries": [
        {
            "text": "Sawubona!",
            "speaker": "A"
        },
        {
            "text": "Sawubona. Ngiyajabula ukukwazisa.",
            "speaker": "B"
        },
        {
            "text": "Ubani igama lakho?",
            "speaker": "A"
        },
        {
            "text": "Igama lami ngingu [Igama]. Wena?",
            "speaker": "B"
        },
        {
            "text": "Igama lami ngingu [Igama]. Umuphi?",
            "speaker": "A"
        },
        {
            "text": "Ngingowase-Amerika. Wena?",
            "speaker": "B"
        },
        {
            "text": "Ngingowase-Korea. Ngiyakujabulela ukukwazi.",
            "speaker": "A"
        },
        {
            "text": "Nami futhi. Hamba kahle.",
            "speaker": "B"
        }
    ]
}