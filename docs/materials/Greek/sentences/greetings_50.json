{
    "subject": "Βασικές Εκφράσεις και Καθημερινές Φράσεις 50",
    "entries": [
        {
            "text": "Γεια σας / Χαίρετε.",
            "note": "Ευγενικός χαιρετισμός"
        },
        {
            "text": "Χάρηκα για τη γνωριμία.",
            "note": "Πρώτη συνάντηση"
        },
        {
            "text": "Καιρό έχουμε να τα πούμε.",
            "note": "Επανασύνδεση"
        },
        {
            "text": "Ευχαριστώ πολύ.",
            "note": "Επίσημο"
        },
        {
            "text": "Λυπάμαι.",
            "note": "Επίσημο"
        },
        {
            "text": "Με συγχωρείτε.",
            "note": "Προσέλκυση προσοχής"
        },
        {
            "text": "Πώς είστε;",
            "note": "Χαιρετισμός"
        },
        {
            "text": "Καλά, ευχαριστώ.",
            "note": ""
        },
        {
            "text": "Πώς σας λένε;",
            "note": ""
        },
        {
            "text": "Το όνομά μου είναι ...",
            "note": ""
        },
        {
            "text": "Από πού είστε;",
            "note": ""
        },
        {
            "text": "Είμαι από την Αμερική.",
            "note": ""
        },
        {
            "text": "Στο επανιδείν.",
            "note": "Σε αυτόν που φεύγει"
        },
        {
            "text": "Στο καλό / Γεια σας.",
            "note": "Σε αυτόν που μένει"
        },
        {
            "text": "Ναι.",
            "note": "Θετικά"
        },
        {
            "text": "Όχι.",
            "note": "Αρνητικά"
        },
        {
            "text": "Ίσως.",
            "note": "Υπόθεση"
        },
        {
            "text": "Καταλαβαίνω.",
            "note": ""
        },
        {
            "text": "Δεν ξέρω.",
            "note": ""
        },
        {
            "text": "Βοηθήστε με, παρακαλώ.",
            "note": ""
        },
        {
            "text": "Περιμένετε ένα λεπτό, παρακαλώ.",
            "note": ""
        },
        {
            "text": "Πόσο κοστίζει αυτό;",
            "note": "Ψώνια"
        },
        {
            "text": "Πού είναι η τουαλέτα;",
            "note": ""
        },
        {
            "text": "Πεινάω.",
            "note": ""
        },
        {
            "text": "Διψάω.",
            "note": ""
        },
        {
            "text": "Είναι νόστιμο.",
            "note": ""
        },
        {
            "text": "Μου αρέσει.",
            "note": ""
        },
        {
            "text": "Είναι εντάξει.",
            "note": ""
        },
        {
            "text": "Καλή τύχη.",
            "note": ""
        },
        {
            "text": "Τι ώρα είναι τώρα;",
            "note": ""
        },
        {
            "text": "Σήμερα ο καιρός είναι καλός.",
            "note": ""
        },
        {
            "text": "Κάνει κρύο.",
            "note": ""
        },
        {
            "text": "Κάνει ζέστη.",
            "note": ""
        },
        {
            "text": "Είμαι κουρασμένος.",
            "note": ""
        },
        {
            "text": "Είμαι ευτυχισμένος.",
            "note": ""
        },
        {
            "text": "Συγχαρητήρια.",
            "note": ""
        },
        {
            "text": "Χρόνια πολλά.",
            "note": ""
        },
        {
            "text": "Να έχετε μια καλή μέρα.",
            "note": ""
        },
        {
            "text": "Θα τα πούμε αύριο.",
            "note": ""
        },
        {
            "text": "Να προσέχετε.",
            "note": ""
        },
        {
            "text": "Μιλάτε αγγλικά;",
            "note": ""
        },
        {
            "text": "Μιλάω μόνο λίγα κορεάτικα.",
            "note": ""
        },
        {
            "text": "Μιλάτε αργά, παρακαλώ.",
            "note": ""
        },
        {
            "text": "Τι σημαίνει αυτό;",
            "note": ""
        },
        {
            "text": "Πώς το λένε στα κορεάτικα;",
            "note": ""
        },
        {
            "text": "Σ' αγαπώ.",
            "note": ""
        },
        {
            "text": "Περιμένετε, παρακαλώ.",
            "note": ""
        },
        {
            "text": "Παρακαλώ / Τίποτα.",
            "note": ""
        },
        {
            "text": "Όχι, ευχαριστώ.",
            "note": ""
        },
        {
            "text": "Έχω μια ερώτηση.",
            "note": ""
        }
    ]
}