{
    "title": "פגישה ראשונה",
    "entries": [
        {
            "text": "שלום!",
            "speaker": "A"
        },
        {
            "text": "שלום. נעים להכיר.",
            "speaker": "B"
        },
        {
            "text": "מה שמך?",
            "speaker": "A"
        },
        {
            "text": "שמי [שם]. ושלך?",
            "speaker": "B"
        },
        {
            "text": "שמי [שם]. מאיפה אתה?",
            "speaker": "A"
        },
        {
            "text": "אני מאמריקה. ואתה?",
            "speaker": "B"
        },
        {
            "text": "אני מקוריאה. נעים להכיר אותך.",
            "speaker": "A"
        },
        {
            "text": "גם אותי. להתראות.",
            "speaker": "B"
        }
    ]
}