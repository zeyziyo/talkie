{
    "subject": "आधारभूत अभिव्यक्ति र दैनिक वाक्य ५०",
    "entries": [
        {
            "text": "नमस्ते / नमस्कार।",
            "note": "शिष्ट अभिवादन"
        },
        {
            "text": "तपाईंलाई भेटेर खुसी लाग्यो।",
            "note": "पहिलो भेट"
        },
        {
            "text": "धेरै समयपछि भेट भयो।",
            "note": "पुनर्मिलन"
        },
        {
            "text": "धेरै धेरै धन्यवाद।",
            "note": "औपचारिक"
        },
        {
            "text": "मलाई माफ गर्नुहोस्।",
            "note": "औपचारिक"
        },
        {
            "text": "सुन्नुहोस् त / माफ गर्नुहोस्।",
            "note": "ध्यान आकर्षण"
        },
        {
            "text": "सन्चै हुनुहुन्छ?",
            "note": "अभिवादन"
        },
        {
            "text": "म सन्चै छु, धन्यवाद।",
            "note": ""
        },
        {
            "text": "तपाईंको नाम के हो?",
            "note": ""
        },
        {
            "text": "मेरो नाम ... हो।",
            "note": ""
        },
        {
            "text": "तपाईं कहाँबाट आउनुभएको हो?",
            "note": ""
        },
        {
            "text": "म अमेरिकाबाट आएको हुँ।",
            "note": ""
        },
        {
            "text": "जान्छु है / बिदा।",
            "note": "जाने व्यक्तिका लागि"
        },
        {
            "text": "राम्रोसँग बस्नुहोला / बिदा।",
            "note": "बस्ने व्यक्तिका लागि"
        },
        {
            "text": "हजुर / हो।",
            "note": "सकारात्मक"
        },
        {
            "text": "होइन।",
            "note": "नकारात्मक"
        },
        {
            "text": "सायद।",
            "note": "अनुमान"
        },
        {
            "text": "मैले बुझें।",
            "note": ""
        },
        {
            "text": "मलाई थाहा छैन।",
            "note": ""
        },
        {
            "text": "कृपया मलाई मद्दत गर्नुहोस्।",
            "note": ""
        },
        {
            "text": "कृपया एकछिन पर्खिनुहोस्।",
            "note": ""
        },
        {
            "text": "यसको मूल्य कति हो?",
            "note": "किनमेल"
        },
        {
            "text": "शौचालय कहाँ छ?",
            "note": ""
        },
        {
            "text": "मलाई भोक लागेको छ।",
            "note": ""
        },
        {
            "text": "मलाई तिर्खा लागेको छ।",
            "note": ""
        },
        {
            "text": "यो स्वादिष्ट छ।",
            "note": ""
        },
        {
            "text": "मलाई यो मन पर्छ।",
            "note": ""
        },
        {
            "text": "ठिकै छ / केही छैन।",
            "note": ""
        },
        {
            "text": "शुभकामना।",
            "note": ""
        },
        {
            "text": "अहिले कति बज्यो?",
            "note": ""
        },
        {
            "text": "आज मौसम राम्रो छ।",
            "note": ""
        },
        {
            "text": "चिसो छ।",
            "note": ""
        },
        {
            "text": "गर्मी छ।",
            "note": ""
        },
        {
            "text": "म थाकेको छु।",
            "note": ""
        },
        {
            "text": "म खुसी छु।",
            "note": ""
        },
        {
            "text": "बधाई छ।",
            "note": ""
        },
        {
            "text": "जन्मदिनको शुभकामना।",
            "note": ""
        },
        {
            "text": "तपाईंको दिन शुभ रहोस्।",
            "note": ""
        },
        {
            "text": "भोलि भेटौंला।",
            "note": ""
        },
        {
            "text": "आफ्नो ख्याल राख्नुहोला।",
            "note": ""
        },
        {
            "text": "के तपाईं अंग्रेजी बोल्नुहुन्छ?",
            "note": ""
        },
        {
            "text": "म कोरियन थोरै मात्र बोल्छु।",
            "note": ""
        },
        {
            "text": "कृपया बिस्तारै बोल्नुहोस्।",
            "note": ""
        },
        {
            "text": "यसको अर्थ के हो?",
            "note": ""
        },
        {
            "text": "यसलाई कोरियनमा के भनिन्छ?",
            "note": ""
        },
        {
            "text": "म तिमीलाई माया गर्छु।",
            "note": ""
        },
        {
            "text": "कृपया पर्खनुहोस्।",
            "note": ""
        },
        {
            "text": "स्वागत छ / केही छैन।",
            "note": ""
        },
        {
            "text": "अहँ, धन्यवाद।",
            "note": ""
        },
        {
            "text": "मेरो एउटा प्रश्न छ।",
            "note": ""
        }
    ]
}