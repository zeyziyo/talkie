{
    "subject": "تعبيرات أساسية وجمل يومية 50",
    "entries": [
        {
            "text": "مرحباً / سلام.",
            "note": "تحية مهذبة"
        },
        {
            "text": "تشرفت بلقائك.",
            "note": "اللقاء الأول"
        },
        {
            "text": "لم نلتقِ منذ فترة طويلة.",
            "note": "لقاء مجدد"
        },
        {
            "text": "شكراً جزيلاً.",
            "note": "رسمي"
        },
        {
            "text": "أنا آسف.",
            "note": "رسمي"
        },
        {
            "text": "معذرة.",
            "note": "لجذب الانتباه"
        },
        {
            "text": "كيف حالك؟",
            "note": "تحية"
        },
        {
            "text": "بخير، شكراً.",
            "note": ""
        },
        {
            "text": "ما اسمك؟",
            "note": ""
        },
        {
            "text": "اسمي ...",
            "note": ""
        },
        {
            "text": "من أين أنت؟",
            "note": ""
        },
        {
            "text": "أنا من أمريكا.",
            "note": ""
        },
        {
            "text": "رافقتك السلامة.",
            "note": "للذاهب"
        },
        {
            "text": "ابقَ بخير / مع السلامة.",
            "note": "للباقي"
        },
        {
            "text": "نعم.",
            "note": "إيجاب"
        },
        {
            "text": "لا.",
            "note": "سلب"
        },
        {
            "text": "ربما.",
            "note": "تخمين"
        },
        {
            "text": "فهمتُ.",
            "note": ""
        },
        {
            "text": "لا أعرف.",
            "note": ""
        },
        {
            "text": "ساعدني من فضلك.",
            "note": ""
        },
        {
            "text": "انتظر لحظة من فضلك.",
            "note": ""
        },
        {
            "text": "بكم هذا؟",
            "note": "تسوق"
        },
        {
            "text": "أين المرحاض؟",
            "note": ""
        },
        {
            "text": "أنا جائع.",
            "note": ""
        },
        {
            "text": "أنا عطشان.",
            "note": ""
        },
        {
            "text": "هذا لذيذ.",
            "note": ""
        },
        {
            "text": "أحب هذا.",
            "note": ""
        },
        {
            "text": "لا بأس / لا مشكلة.",
            "note": ""
        },
        {
            "text": "بالتوفيق.",
            "note": ""
        },
        {
            "text": "كم الساعة الآن؟",
            "note": ""
        },
        {
            "text": "الطقس جميل اليوم.",
            "note": ""
        },
        {
            "text": "جو بارد.",
            "note": ""
        },
        {
            "text": "جو حار.",
            "note": ""
        },
        {
            "text": "أنا متعب.",
            "note": ""
        },
        {
            "text": "أنا سعيد.",
            "note": ""
        },
        {
            "text": "تهانينا.",
            "note": ""
        },
        {
            "text": "عيد ميلاد سعيد.",
            "note": ""
        },
        {
            "text": "أتمنى لك يوماً سعيداً.",
            "note": ""
        },
        {
            "text": "أراك غداً.",
            "note": ""
        },
        {
            "text": "اعتنِ بنفسك.",
            "note": ""
        },
        {
            "text": "هل تتحدث الإنجليزية؟",
            "note": ""
        },
        {
            "text": "أتحدث القليل من الكورية فقط.",
            "note": ""
        },
        {
            "text": "تحدث ببطء من فضلك.",
            "note": ""
        },
        {
            "text": "ما معنى هذا؟",
            "note": ""
        },
        {
            "text": "ماذا يسمى هذا بالكورية؟",
            "note": ""
        },
        {
            "text": "أحبك.",
            "note": ""
        },
        {
            "text": "انتظر من فضلك.",
            "note": ""
        },
        {
            "text": "عفواً / لا بأس.",
            "note": ""
        },
        {
            "text": "لا، شكراً.",
            "note": ""
        },
        {
            "text": "لدي سؤال.",
            "note": ""
        }
    ]
}