{
    "subject": "Alapvető kifejezések és Mindennapi Mondatok 50",
    "entries": [
        {
            "text": "Jó napot / Szia.",
            "note": "Udvarias üdvözlés"
        },
        {
            "text": "Örülök, hogy megismertem.",
            "note": "Első találkozás"
        },
        {
            "text": "Rég láttuk egymást.",
            "note": "Újbóli találkozás"
        },
        {
            "text": "Köszönöm szépen.",
            "note": "Formális"
        },
        {
            "text": "Sajnálom.",
            "note": "Formális"
        },
        {
            "text": "Elnézést.",
            "note": "Figyelemfelkeltés"
        },
        {
            "text": "Hogy van?",
            "note": "Üdvözlés"
        },
        {
            "text": "Jól vagyok, köszönöm.",
            "note": ""
        },
        {
            "text": "Mi a neve?",
            "note": ""
        },
        {
            "text": "A nevem ...",
            "note": ""
        },
        {
            "text": "Hová valósi?",
            "note": ""
        },
        {
            "text": "Amerikából jöttem.",
            "note": ""
        },
        {
            "text": "Viszontlátásra.",
            "note": "Távozónak"
        },
        {
            "text": "Minden jót / Viszlát.",
            "note": "Maradónak"
        },
        {
            "text": "Igen.",
            "note": "Pozitív"
        },
        {
            "text": "Nem.",
            "note": "Negatív"
        },
        {
            "text": "Talán.",
            "note": "Feltételezés"
        },
        {
            "text": "Értem.",
            "note": ""
        },
        {
            "text": "Nem tudom.",
            "note": ""
        },
        {
            "text": "Segítsen kérem.",
            "note": ""
        },
        {
            "text": "Várjon egy pillanatot kérem.",
            "note": ""
        },
        {
            "text": "Mennyibe kerül ez?",
            "note": "Vásárlás"
        },
        {
            "text": "Merre van a mosdó?",
            "note": ""
        },
        {
            "text": "Éhes vagyok.",
            "note": ""
        },
        {
            "text": "Szomjas vagyok.",
            "note": ""
        },
        {
            "text": "Finom.",
            "note": ""
        },
        {
            "text": "Szeretem.",
            "note": ""
        },
        {
            "text": "Rendben van.",
            "note": ""
        },
        {
            "text": "Sok szerencsét.",
            "note": ""
        },
        {
            "text": "Mennyi az idő most?",
            "note": ""
        },
        {
            "text": "Jó idő van ma.",
            "note": ""
        },
        {
            "text": "Hideg van.",
            "note": ""
        },
        {
            "text": "Meleg van.",
            "note": ""
        },
        {
            "text": "Fáradt vagyok.",
            "note": ""
        },
        {
            "text": "Boldog vagyok.",
            "note": ""
        },
        {
            "text": "Gratulálok.",
            "note": ""
        },
        {
            "text": "Boldog születésnapot.",
            "note": ""
        },
        {
            "text": "Szép napot.",
            "note": ""
        },
        {
            "text": "Holnap találkozunk.",
            "note": ""
        },
        {
            "text": "Vigyázzon magára.",
            "note": ""
        },
        {
            "text": "Beszél angolul?",
            "note": ""
        },
        {
            "text": "Csak egy kicsit beszélek koreaiul.",
            "note": ""
        },
        {
            "text": "Kérem, beszéljen lassan.",
            "note": ""
        },
        {
            "text": "Ez mit jelent?",
            "note": ""
        },
        {
            "text": "Hogy mondják koreaiul?",
            "note": ""
        },
        {
            "text": "Szeretlek.",
            "note": ""
        },
        {
            "text": "Kérem, várjon.",
            "note": ""
        },
        {
            "text": "Szívesen / Nincs mit.",
            "note": ""
        },
        {
            "text": "Nem, köszönöm.",
            "note": ""
        },
        {
            "text": "Van egy kérdésem.",
            "note": ""
        }
    ]
