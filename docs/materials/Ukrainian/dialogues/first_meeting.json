{
    "title": "Перша зустріч",
    "entries": [
        {
            "text": "Вітаю!",
            "speaker": "A"
        },
        {
            "text": "Вітаю. Радий зустрічі.",
            "speaker": "B"
        },
        {
            "text": "Як вас звати?",
            "speaker": "A"
        },
        {
            "text": "Мене звати [Ім'я]. А вас?",
            "speaker": "B"
        },
        {
            "text": "Мене звати [Ім'я]. Звідки ви?",
            "speaker": "A"
        },
        {
            "text": "Я з Америки. А ви?",
            "speaker": "B"
        },
        {
            "text": "Я з Кореї. Радий був познайомитися.",
            "speaker": "A"
        },
        {
            "text": "Я також. До побачення.",
            "speaker": "B"
        }
    ]
}