    "subject": "Temel İfadeler ve Günlük Cümleler 50",
    "entries": [
        {
            "text": "Merhaba.",
            "note": "Kibar selamlama"
        },
        {
            "text": "Tanıştığımıza memnun oldum.",
            "note": "İlk karşılaşma"
        },
        {
            "text": "Görüşmeyeli uzun zaman oldu.",
            "note": "Tekrar görüşme"
        },
        {
            "text": "Çok teşekkür ederim.",
            "note": "Resmi"
        },
        {
            "text": "Özür dilerim.",
            "note": "Resmi"
        },
        {
            "text": "Affedersiniz.",
            "note": "Dikkat çekmek için"
        },
        {
            "text": "Nasılsınız?",
            "note": "Selamlama"
        },
        {
            "text": "İyiyim, teşekkürler.",
            "note": ""
        },
        {
            "text": "Adınız nedir?",
            "note": ""
        },
        {
            "text": "Benim adım ...",
            "note": ""
        },
        {
            "text": "Nerelisiniz?",
            "note": ""
        },
        {
            "text": "Amerikalıyım.",
            "note": ""
        },
        {
            "text": "Güle güle.",
            "note": "Gidena söylenir"
        },
        {
            "text": "Hoşça kalın.",
            "note": "Kalana söylenir"
        },
        {
            "text": "Evet.",
            "note": "Olumlu"
        },
        {
            "text": "Hayır.",
            "note": "Olumsuz"
        },
        {
            "text": "Belki.",
            "note": "Tahmin"
        },
        {
            "text": "Anladım.",
            "note": ""
        },
        {
            "text": "Bilmiyorum.",
            "note": ""
        },
        {
            "text": "Lütfen bana yardım edin.",
            "note": ""
        },
        {
            "text": "Lütfen biraz bekleyin.",
            "note": ""
        },
        {
            "text": "Bu ne kadar?",
            "note": "Alışveriş"
        },
        {
            "text": "Tuvalet nerede?",
            "note": ""
        },
        {
            "text": "Acıktım.",
            "note": ""
        },
        {
            "text": "Susadım.",
            "note": ""
        },
        {
            "text": "Bu lezzetli.",
            "note": ""
        },
        {
            "text": "Buna bayıldım.",
            "note": ""
        },
        {
            "text": "Sorun değil / Rica ederim.",
            "note": ""
        },
        {
            "text": "Başarılar dilerim.",
            "note": ""
        },
        {
            "text": "Saat kaç?",
            "note": ""
        },
        {
            "text": "Bugün hava çok güzel.",
            "note": ""
        },
        {
            "text": "Soğuk.",
            "note": ""
        },
        {
            "text": "Sıcak.",
            "note": ""
        },
        {
            "text": "Yorgunum.",
            "note": ""
        },
        {
            "text": "Mutluyum.",
            "note": ""
        },
        {
            "text": "Tebrikler.",
            "note": ""
        },
        {
            "text": "Doğum günün kutlu olsun.",
            "note": ""
        },
        {
            "text": "İyi günler dilerim.",
            "note": ""
        },
        {
            "text": "Yarın görüşürüz.",
            "note": ""
        },
        {
            "text": "Kendine iyi bak.",
            "note": ""
        },
        {
            "text": "İngilizce biliyor musunuz?",
            "note": ""
        },
        {
            "text": "Sadece biraz Korece biliyorum.",
            "note": ""
        },
        {
            "text": "Lütfen yavaş konuşun.",
            "note": ""
        },
        {
            "text": "Bu ne demek?",
            "note": ""
        },
        {
            "text": "Buna Korece'de ne denir?",
            "note": ""
        },
        {
            "text": "Seni seviyorum.",
            "note": ""
        },
        {
            "text": "Lütfen bekleyin.",
            "note": ""
        },
        {
            "text": "Rica ederim / Sorun değil.",
            "note": ""
        },
        {
            "text": "Hayır, teşekkürler.",
            "note": ""
        },
        {
            "text": "Bir sorum var.",
            "note": ""
