{
    "subject": "बुनियादी बातें और दैनिक वाक्य 50",
    "entries": [
        {
            "text": "नमस्ते / नमस्कार।",
            "note": "विनम्र अभिवादन"
        },
        {
            "text": "आपसे मिलकर अच्छा लगा।",
            "note": "पहली मुलाकात"
        },
        {
            "text": "बहुत समय बाद मिले।",
            "note": "पुनर्मिलन"
        },
        {
            "text": "बहुत बहुत धन्यवाद।",
            "note": "मर्यादापूर्ण"
        },
        {
            "text": "मुझे क्षमा करें।",
            "note": "मर्यादापूर्ण"
        },
        {
            "text": "सुनिए / क्षमा करें।",
            "note": "ध्यान आकर्षित करना"
        },
        {
            "text": "आप कैसे हैं?",
            "note": "हाल-चाल"
        },
        {
            "text": "मैं ठीक हूँ, धन्यवाद।",
            "note": ""
        },
        {
            "text": "आपका नाम क्या है?",
            "note": ""
        },
        {
            "text": "मेरा नाम ... है।",
            "note": ""
        },
        {
            "text": "आप कहाँ से हैं?",
            "note": ""
        },
        {
            "text": "मैं अमेरिका से हूँ।",
            "note": ""
        },
        {
            "text": "नमस्ते / अलविदा।",
            "note": "जाने वाले को"
        },
        {
            "text": "नमस्ते / फिर मिलेंगे।",
            "note": "रहने वाले को"
        },
        {
            "text": "हाँ।",
            "note": "सकारात्मक"
        },
        {
            "text": "नहीं।",
            "note": "नकारात्मक"
        },
        {
            "text": "शायद।",
            "note": "अनुमान"
        },
        {
            "text": "मैं समझता हूँ।",
            "note": ""
        },
        {
            "text": "मुझे नहीं पता।",
            "note": ""
        },
        {
            "text": "कृपया मेरी मदद करें।",
            "note": ""
        },
        {
            "text": "कृपया एक पल रुकिए।",
            "note": ""
        },
        {
            "text": "यह कितने का है?",
            "note": "खरीदारी"
        },
        {
            "text": "शौचालय कहाँ है?",
            "note": ""
        },
        {
            "text": "मुझे भूख लगी है।",
            "note": ""
        },
        {
            "text": "मुझे प्यास लगी है।",
            "note": ""
        },
        {
            "text": "यह स्वादिष्ट है।",
            "note": ""
        },
        {
            "text": "मुझे यह पसंद है।",
            "note": ""
        },
        {
            "text": "कोई बात नहीं / ठीक है।",
            "note": ""
        },
        {
            "text": "शुभकामनाएं।",
            "note": ""
        },
        {
            "text": "अभी क्या समय हुआ है?",
            "note": ""
        },
        {
            "text": "आज मौसम अच्छा है।",
            "note": ""
        },
        {
            "text": "ठंड है।",
            "note": ""
        },
        {
            "text": "गर्मी है।",
            "note": ""
        },
        {
            "text": "मैं थक गया हूँ।",
            "note": ""
        },
        {
            "text": "मैं खुश हूँ।",
            "note": ""
        },
        {
            "text": "बधाई हो।",
            "note": ""
        },
        {
            "text": "जन्मदिन मुबारक हो।",
            "note": ""
        },
        {
            "text": "आपका दिन शुभ हो।",
            "note": ""
        },
        {
            "text": "कल मिलते हैं।",
            "note": ""
        },
        {
            "text": "अपना ख्याल रखना।",
            "note": ""
        },
        {
            "text": "क्या आप अंग्रेजी बोलते हैं?",
            "note": ""
        },
        {
            "text": "मैं केवल थोड़ी कोरियाई बोलता हूँ।",
            "note": ""
        },
        {
            "text": "कृपया धीरे बोलें।",
            "note": ""
        },
        {
            "text": "इसका क्या मतलब है?",
            "note": ""
        },
        {
            "text": "कोरियाई में इसे क्या कहते हैं?",
            "note": ""
        },
        {
            "text": "मैं आपसे प्यार करता हूँ।",
            "note": ""
        },
        {
            "text": "कृपया प्रतीक्षा करें।",
            "note": ""
        },
        {
            "text": "आपका स्वागत है / कोई बात नहीं।",
            "note": ""
        },
        {
            "text": "जी नहीं, धन्यवाद।",
            "note": ""
        },
        {
            "text": "मेरा एक प्रश्न है।",
            "note": ""
        }
    ]
}