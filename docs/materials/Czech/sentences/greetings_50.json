{
    "subject": "Základní výrazy a každodenní věty 50",
    "entries": [
        {
            "text": "Dobrý den / Ahoj.",
            "note": "Zdvořilý pozdrav"
        },
        {
            "text": "Rád vás poznávám.",
            "note": "První setkání"
        },
        {
            "text": "Dlouho jsme se neviděli.",
            "note": "Opětovné setkání"
        },
        {
            "text": "Děkuji mockrát.",
            "note": "Formálně"
        },
        {
            "text": "Omlouvám se.",
            "note": "Formálně"
        },
        {
            "text": "S dovolením.",
            "note": "Upoutání pozornosti"
        },
        {
            "text": "Jak se máte?",
            "note": "Pozdrav"
        },
        {
            "text": "Dobře, děkuji.",
            "note": ""
        },
        {
            "text": "Jak se jmenujete?",
            "note": ""
        },
        {
            "text": "Jmenuji se ...",
            "note": ""
        },
        {
            "text": "Odkud jste?",
            "note": ""
        },
        {
            "text": "Jsem z Ameriky.",
            "note": ""
        },
        {
            "text": "Na shledanou.",
            "note": "Tomu, kdo odchází"
        },
        {
            "text": "Mějte se hezky.",
            "note": "Tomu, kdo zůstává"
        },
        {
            "text": "Ano.",
            "note": "Kladně"
        },
        {
            "text": "Ne.",
            "note": "Záporně"
        },
        {
            "text": "Možná.",
            "note": "Domněnka"
        },
        {
            "text": "Rozumím.",
            "note": ""
        },
        {
            "text": "Nevím.",
            "note": ""
        },
        {
            "text": "Pomozte mi, prosím.",
            "note": ""
        },
        {
            "text": "Počkejte chvíli, prosím.",
            "note": ""
        },
        {
            "text": "Kolik to stojí?",
            "note": "Nakupování"
        },
        {
            "text": "Kde je toaleta?",
            "note": ""
        },
        {
            "text": "Mám hlad.",
            "note": ""
        },
        {
            "text": "Mám žízeň.",
            "note": ""
        },
        {
            "text": "Je to chutné.",
            "note": ""
        },
        {
            "text": "Líbí se mi to.",
            "note": ""
        },
        {
            "text": "Je to v pořádku.",
            "note": ""
        },
        {
            "text": "Hodně štěstí.",
            "note": ""
        },
        {
            "text": "Kolik je teď hodin?",
            "note": ""
        },
        {
            "text": "Dnes je hezké počasí.",
            "note": ""
        },
        {
            "text": "Je zima.",
            "note": ""
        },
        {
            "text": "Je horko.",
            "note": ""
        },
        {
            "text": "Jsem unavený.",
            "note": ""
        },
        {
            "text": "Jsem šťastný.",
            "note": ""
        },
        {
            "text": "Gratuluji.",
            "note": ""
        },
        {
            "text": "Všechno nejlepší k narozeninám.",
            "note": ""
        },
        {
            "text": "Hezký den.",
            "note": ""
        },
        {
            "text": "Uvidíme se zítra.",
            "note": ""
        },
        {
            "text": "Opatrujte se.",
            "note": ""
        },
        {
            "text": "Mluvíte anglicky?",
            "note": ""
        },
        {
            "text": "Mluvím jen trochu korejsky.",
            "note": ""
        },
        {
            "text": "Mluvte pomalu, prosím.",
            "note": ""
        },
        {
            "text": "Co to znamená?",
            "note": ""
        },
        {
            "text": "Jak se to řekne korejsky?",
            "note": ""
        },
        {
            "text": "Miluji tě.",
            "note": ""
        },
        {
            "text": "Počkejte, prosím.",
            "note": ""
        },
        {
            "text": "Není zač.",
            "note": ""
        },
        {
            "text": "Ne, děkuji.",
            "note": ""
        },
        {
            "text": "Mám otázku.",
            "note": ""
        }
    ]
}